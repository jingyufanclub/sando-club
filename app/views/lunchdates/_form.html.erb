<script type="text/javascript">
  function initialize() {
    const input = document.querySelector('#place');
    const autocomplete = new google.maps.places.Autocomplete(input);
  }
  google.maps.event.addDomListener(window, 'load', initialize);
</script>



<%= form_with model: @lunchdate, local: true do |f|%>
  <%= f.date_select :date %><br><br>
  <%= f.time_select :time, {start_hour: 11, end_hour: 15, ampm: true, minute_step: 15} %><br><br>
  <%= f.label :place, "Location" %>
  <%= f.text_field :place, id: "place" %><br><br>
  <%= f.label :group_id %>
  <%= f.select(:group_id, @groups.map {|g| [g.name, g.id]}) %><br><br>
  <%= f.hidden_field :longitude %>
  <%= f.hidden_field :latitude %>
  <%= f.submit 'Schedule This Lunch'%>
<% end %>
